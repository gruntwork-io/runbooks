  "dependencies": {
    "@astrojs/starlight": "^0.36.0",
    "@vercel/analytics": "^1.5.0",
    "astro": "^5.6.1",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "cspell": "^9.4.0",
    "linkinator": "^7.5.1"
  }
}
