{
  "name": "molecular-mercury",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "cd ../testdata && zip -r ../docs/public/my-first-runbook.zip my-first-runbook",
